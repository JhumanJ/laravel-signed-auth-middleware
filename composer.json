{
    "name": "jhumanj/laravel-signed-auth-middleware",
    "description": "A Laravel package that can authenticate users using signed links",
    "keywords": [
        "jhumanj",
        "laravel-signed-auth-middleware"
    ],
    "homepage": "https://github.com/jhumanj/laravel-signed-auth-middleware",
    "license": "MIT",
    "authors": [
        {
            "name": "JhumanJ",
            "email": "julien@nahum.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "spatie/laravel-package-tools": "^1.1",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.12",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "spatie/ray": "^1.19",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "JhumanJ\\LaravelSignedAuthMiddleware\\": "src",
            "JhumanJ\\LaravelSignedAuthMiddleware\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JhumanJ\\LaravelSignedAuthMiddleware\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SignedAuthMiddlewareServiceProvider"
            ],
            "aliases": {
                "LaravelSignedAuthMiddleware": "SignedAuthMiddlewareFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
